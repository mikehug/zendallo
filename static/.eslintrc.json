{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "parser": "babel-eslint",  
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react"],
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "react/jsx-filename-extension": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "react/require-default-props": 0,
    "react/prop-types": 0
  }
}
